    "eslint": "^1.9.0",
    "eslint-config-cycle": "^3.0.0",
    "eslint-plugin-cycle": "^1.0.2",
    "faucet": "0.0.1",
    "live-server": "^0.8.2",
    "markdox": "^0.1.10",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^1.2.13",
    "opener": "^1.4.1",
    "tape": "^4.2.2",
    "uglify-js": "^2.6.0",
    "watchify": "^3.6.0",
    "zuul": "^3.7.2"
  },
  "browserify-shim": {
    "rx": "global:Rx"
