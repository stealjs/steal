module("<%= underscore %>")

test("findAll", function(){
   <%= className %>.findAll({}, function(){
        ok(<%= plural %>, "<%= plural %> exists")
        ok(<%= plural %>.length, "<%= plural %> has length")
        ok(<%= plural %>[0].name, "<%= plural %> has a name")
        ok(<%= plural %>[0].description, "<%= plural %> has a description")
   })
})
